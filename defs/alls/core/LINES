N=6
perl -pi -e 's/"line${N}":/"line_${N}":/gmi' *.json
perl -pi -e 's/"line ${N}":/"line_${N}":/gmi' *.json
perl -pi -e 's/"line_${N}":/"line_${N}":/gmi' *.json
perl -pi -e 's/"Line_${N}":/"line_${N}":/gmi' *.json
perl -pi -e 's/"Line ${N}":/"line_${N}":/gmi' *.json
perl -pi -e 's/"Line /"line_/gmi' *.json


